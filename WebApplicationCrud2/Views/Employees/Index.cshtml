@model List<WebApplicationCrud2.Models.Domain.Employee>
 <h1>Employees</h1>
 <table>

     <thead
         <tr>
             <th>id</th>
             <th>name</th>
             <th>email</th>
             <th>salary</th>
         </tr>
     </thead>
     <tbody>
         @foreach(var employee in Model)
         {
         <tr>
            <td>@employee.Id</td>
            <td>@employee.Name</td>
            <td>@employee.Email</td>
            <td>@employee.Salary</td>
         </tr>
           }

     </tbody>
 </table>